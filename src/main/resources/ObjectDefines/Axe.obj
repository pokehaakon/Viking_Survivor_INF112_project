

$Animation = Define Animation:
    State: IDLE axe.gif
    state: MOVING axe.gif

$Stats = Define Stats:
    HP: 0
    Speed: 100.0
    Damage: 10.0
    resistance: 0.0
    scale: 15

$Shape = Define Shape:
    Type: Circle
    Radius: 2


$Structure = Define Structure:
    Filter: ENEMY
    Category: WEAPON
    Shape: $Shape
    BodyType: DynamicBody
    density: 1
    friction: 0
    isSensor: true

$AXE = Define Actor:
    Animation: $Animation
    Stats: $Stats
    Structure: $Structure