$Animation = Define Animation:
    State: IDLE tree.png
    Scale: 20


$Shape = Define Shape:
    Type: Circle
    Radius: 7.0

$Structure = Define Structure:
    Filter: ENEMY PLAYER
    Category: WALL
    Shape: $Shape
    BodyType: StaticBody
    density: 1
    friction: 1
    isSensor: false

$TREE = Define Object:
    Animation: $Animation
    Structure: $Structure