    Filter: ENEMY PLAYER WEAPON WALL
    Category: ENEMY
    Shape: $Shape
    BodyType: DynamicBody
    density: 2.0
    friction: 0
    isSensor: false

$WOLF = Define Actor:
    Animation: $Animation
    Stats: $Stats
    Structure: $Structure
